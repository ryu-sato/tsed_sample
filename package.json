{
  "name": "tsed-sample",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "yarn run tsc && babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "tsc": "tsc --project tsconfig.compile.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start": "babel-watch --extensions .ts src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "bundle": "yarn run tsc && cross-env NODE_ENV=production webpack",
    "start:bundle": "cross-env NODE_ENV=production node dist/app.bundle.js",
    "test": "yarn run test:lint && yarn run test:coverage",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:coverage": "yarn run test:unit",
    "test:lint": "eslint src --ext .ts",
    "test:lint:fix": "eslint src --ext .ts --fix",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "prisma:generate": "npx prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^2.26.0",
    "@tsed/ajv": "^6.59.3",
    "@tsed/common": "^6.59.3",
    "@tsed/core": "^6.59.3",
    "@tsed/di": "^6.59.3",
    "@tsed/exceptions": "^6.59.3",
    "@tsed/json-mapper": "^6.59.3",
    "@tsed/platform-express": "^6.59.3",
    "@tsed/schema": "^6.59.3",
    "@tsed/swagger": "^6.59.3",
    "ajv": "^8.6.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "method-override": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.7",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@tsed/cli-plugin-eslint": "3.4.3",
    "@tsed/cli-plugin-jest": "3.4.3",
    "@tsed/cli-plugin-prisma": "3.4.3",
    "@types/compression": "^1.7.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/method-override": "^0.0.32",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.3.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "babel-watch": "^7.4.1",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.1.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.7.2"
  },
  "tsed": {
    "packageManager": "yarn",
    "convention": "default"
  }
}
